<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<LangVersion>11</LangVersion>
		<Nullable>annotations</Nullable>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<Optimize>true</Optimize>
		<OutputType>library</OutputType>
		<RootNamespace>ConsoleUtils</RootNamespace>
		<Authors>FireDragon91245</Authors>
		<AssemblyVersion>1.2.1</AssemblyVersion>
		<FileVersion>$(AssemblyVersion)</FileVersion>
		<RepositoryType>git</RepositoryType>
		<RepositoryUrl>https://github.com/FireDragon91245/ConsoleUtileties</RepositoryUrl>
		<Version>$(AssemblyVersion)</Version>
		<AssemblyName>$(RootNamespace)V$(AssemblyVersion)</AssemblyName>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
	  <DebugType>embedded</DebugType>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
	  <DebugType>embedded</DebugType>
	</PropertyGroup>

</Project>
